<h1>New Bookmark</h1>

<form action="/create_bookmark">
  <div>
    <label for="name">name</label><br />
    <input id="name" name="name" type="text" placeholder="enter a website name"/>
  </div>
  <div>
    <label for="url">url</label><br />
    <input id="url" name="url" type="text" placeholder="enter a website url"/>
  </div>
  <div>
    <label for="description">description</label><br />
    <input id="description" name="description" type="text" placeholder="enter a description of the website"/>
  </div>
  <div>
    <label for="user">user</label><br />
    <input id="user" name="user_id" type="text" placeholder="enter a user id"/>
  </div>
  <div>
    <input type="submit">
  </div>
</form>

